<?xml version="1.0" encoding="UTF-8"?>

<entity-mapping-config>

	<outputs>
		<!--RDF creation in a local database, at least the output of type "triplestore" must be defined, files are optional-->
		<output type="triplestore" path="/usr/local/vivo/home/tdbContentModels" reset="true" graph="http://vitro.mannlib.cornell.edu/default/vitro-kb-2"/>
		<!--<output type="file" path="/tmp/rdf/" name="brcris_data.nt" format="NT"/>-->
		<!--<output type="file" path="/tmp/rdf/" name="brcris_data.n3" format="N3"/>-->
		<!--RDF creation and immediate transfer to a target destination, the output of type "post" must be defined-->
		<output type="post" url="http://127.0.0.1:8081/vivo/api/sparqlUpdate" user="vivo_root@mydomain.edu" password="vivopwd" graph="&lt;http://vitro.mannlib.cornell.edu/default/vitro-kb-2&gt;"/>
	</outputs>

	<namespaces>
		<namespace prefix="local" url="https://brcris.ibict.br/individual/"/><!--individuals-->
		<namespace prefix="brcris" url="https://brcris.ibict.br/ontology/"/><!--ontology-->
		<namespace prefix="owl" url="http://www.w3.org/2002/07/owl#"/>
		<namespace prefix="rdfs" url="http://www.w3.org/2000/01/rdf-schema#"/>
		<namespace prefix="xsd" url="http://www.w3.org/2001/XMLSchema#"/>
		<namespace prefix="orcid" url="https://orcid.org/"/>
		<namespace prefix="foaf" url="http://xmlns.com/foaf/0.1/"/>
		<namespace prefix="vcard" url="http://www.w3.org/2006/vcard/ns#"/>
		<namespace prefix="vivo" url="http://vivoweb.org/ontology/core#"/>
		<namespace prefix="obo" url="http://purl.obolibrary.org/obo/"/>
		<namespace prefix="geo" url="http://aims.fao.org/aos/geopolitical.owl#"/>
		<namespace prefix="skos" url="http://www.w3.org/2004/02/skos/core#"/>
		<namespace prefix="bibo" url="http://purl.org/ontology/bibo/"/>
		<namespace prefix="biro" url="http://purl.org/spar/biro/"/>
		<namespace prefix="dcterms" url="http://purl.org/dc/terms/"/>
		<namespace prefix="event" url="http://purl.org/NET/c4dm/event.owl#"/>
	</namespaces>

	<source-entities>

		<source-entity type="Person">

			<source-attributes>

				<source-attribute name="identifier.brcris">
					<target-triples>
						<triple>
							<subject type="foaf:Person" namespace="local" prefix="pers_" idType="UUID"/>
							<predicate type="dataProperty" value="brcris:brcrisIdentifier"/>
							<object value="$value"/>
						</triple>
					</target-triples>
				</source-attribute>

				<source-attribute name="identifier.lattes">
					<target-triples>
						<triple>
							<subject type="foaf:Person" namespace="local" prefix="pers_" idType="UUID"/>
							<predicate type="dataProperty" value="brcris:lattesIdentifier"/>
							<object value="$value"/>
						</triple>
					</target-triples>
				</source-attribute>

				<source-attribute name="identifier.lattesShort">
					<target-triples>
						<triple>
							<subject type="foaf:Person" namespace="local" prefix="pers_" idType="UUID"/>
							<predicate type="dataProperty" value="brcris:lattesShortIdentifier"/>
							<object value="$value"/>
						</triple>
					</target-triples>
				</source-attribute>

				<source-attribute name="identifier.cpf">
					<target-triples>
						<triple>
							<subject type="foaf:Person" namespace="local" prefix="pers_" idType="UUID"/>
							<predicate type="dataProperty" value="brcris:cpfNumber"/>
							<object value="$value"/>
						</triple>
					</target-triples>
				</source-attribute>

				<source-attribute name="identifier.email">
					<target-triples>
						<triple>
							<subject type="foaf:Person" namespace="local" prefix="pers_" idType="UUID"/>
							<predicate type="objectProperty" value="obo:ARG_2000028"/>
							<object type="vcard:Individual" namespace="local" prefix="ind_" idType="UUID"/>
						</triple>
						<triple>
							<subject type="vcard:Individual" namespace="local" prefix="ind_" idType="UUID"/>
							<predicate type="objectProperty" value="vcard:hasEmail"/>
							<object type="vcard:Email" namespace="local" prefix="email_" idType="UUID"/>
						</triple>
						<triple>
							<subject type="vcard:Email" namespace="local" prefix="email_" idType="UUID"/>
							<predicate type="dataProperty" value="vcard:email"/>
							<object value="$value"/>
						</triple>
					</target-triples>
				</source-attribute>

				<source-attribute name="identifier.orcid">
					<target-triples>
						<triple>
							<subject type="foaf:Person" namespace="local" prefix="pers_" idType="UUID"/>
							<predicate type="objectProperty" value="vivo:orcidId"/>
							<object type="owl:Thing" namespace="orcid" idType="$value"/>
						</triple>
						<triple>
							<subject type="owl:Thing" namespace="orcid" idType="$value"/>
							<predicate type="dataProperty" value="rdfs:label"/>
							<object value="$value"/>
						</triple>
						<!--For correct visualization in VIVO, inverse property must also be populated-->
						<triple>
							<subject type="owl:Thing" namespace="orcid" idType="$value"/>
							<predicate type="objectProperty" value="vivo:confirmedOrcidId"/>
							<object type="foaf:Person" namespace="local" prefix="pers_" idType="UUID"/>
						</triple>
					</target-triples>
				</source-attribute>

				<source-attribute name="identifier.researcherID">
					<target-triples>
						<triple>
							<subject type="foaf:Person" namespace="local" prefix="pers_" idType="UUID"/>
							<predicate type="dataProperty" value="vivo:researcherId"/>
							<object value="$value"/>
						</triple>
					</target-triples>
				</source-attribute>

				<source-attribute name="identifier.scopusAuthorID">
					<target-triples>
						<triple>
							<subject type="foaf:Person" namespace="local" prefix="pers_" idType="UUID"/>
							<predicate type="dataProperty" value="vivo:scopusId"/>
							<object value="$value"/>
						</triple>
					</target-triples>
				</source-attribute>

				<source-attribute name="identifier.cienciaID">
					<target-triples>
						<triple>
							<subject type="foaf:Person" namespace="local" prefix="pers_" idType="UUID"/>
							<predicate type="dataProperty" value="brcris:cienciaId"/>
							<object value="$value"/>
						</triple>
					</target-triples>
				</source-attribute>

				<source-attribute name="identifier.capes">
					<target-triples>
						<triple>
							<subject type="foaf:Person" namespace="local" prefix="pers_" idType="UUID"/>
							<predicate type="dataProperty" value="brcris:capesIdentifier"/>
							<object value="$value"/>
						</triple>
					</target-triples>
				</source-attribute>

				<source-attribute name="identifier.other">
					<target-triples>
						<triple>
							<subject type="foaf:Person" namespace="local" prefix="pers_" idType="UUID"/>
							<predicate type="dataProperty" value="vivo:identifier"/>
							<object value="$value"/>
						</triple>
					</target-triples>
				</source-attribute>

				<source-attribute name="name" filter="longest-string" filter-limit="1" filter-regex-clean=",\s|Prof(a)*(\.)*|PROF(A)*(\.)*|Dr(a)*(\.)*|DR(A)*(\.)*|Âª(\.)*|\?+">
					<target-triples>
						<triple>
							<subject type="foaf:Person" namespace="local" prefix="pers_" idType="UUID"/>
							<predicate type="dataProperty" value="rdfs:label"/>
							<object value="$value"/>
						</triple>
						<!-- Fill in also the vcard "formattedName" property so the full name attribute displays on VIVO -->
						<triple>
							<subject type="foaf:Person" namespace="local" prefix="pers_" idType="UUID"/>
							<predicate type="objectProperty" value="obo:ARG_2000028"/>
							<object type="vcard:Individual" namespace="local" prefix="ind_" idType="UUID"/>
						</triple>
						<triple>
							<subject type="vcard:Individual" namespace="local" prefix="ind_" idType="UUID"/>
							<predicate type="objectProperty" value="vcard:hasName"/>
							<object type="vcard:Name" namespace="local" prefix="name_" idType="UUID"/>
						</triple>
						<triple>
							<subject type="vcard:Name" namespace="local" prefix="name_" idType="UUID"/>
							<predicate type="dataProperty" value="vcard:formattedName"/>
							<object value="$value"/>
						</triple>
					</target-triples>
				</source-attribute>

				<source-attribute name="fullName">
					<sub-attributes>
						<source-attribute name="familyName">
							<target-triples>
								<triple>
									<subject type="foaf:Person" namespace="local" prefix="pers_" idType="UUID"/>
									<predicate type="objectProperty" value="obo:ARG_2000028"/>
									<object type="vcard:Individual" namespace="local" prefix="ind_" idType="UUID"/>
								</triple>
								<triple>
									<subject type="vcard:Individual" namespace="local" prefix="ind_" idType="UUID"/>
									<predicate type="objectProperty" value="vcard:hasName"/>
									<object type="vcard:Name" namespace="local" prefix="name_" idType="UUID"/>
								</triple>
								<triple>
									<subject type="vcard:Name" namespace="local" prefix="name_" idType="UUID"/>
									<predicate type="dataProperty" value="vcard:familyName"/>
									<object value="$value"/>
								</triple>
							</target-triples>
						</source-attribute>

						<source-attribute name="givenName">
							<target-triples>
								<triple>
									<subject type="foaf:Person" namespace="local" prefix="pers_" idType="UUID"/>
									<predicate type="objectProperty" value="obo:ARG_2000028"/>
									<object type="vcard:Individual" namespace="local" prefix="ind_" idType="UUID"/>
								</triple>
								<triple>
									<subject type="vcard:Individual" namespace="local" prefix="ind_" idType="UUID"/>
									<predicate type="objectProperty" value="vcard:hasName"/>
									<object type="vcard:Name" namespace="local" prefix="name_" idType="UUID"/>
								</triple>
								<triple>
									<subject type="vcard:Name" namespace="local" prefix="name_" idType="UUID"/>
									<predicate type="dataProperty" value="vcard:givenName"/>
									<object value="$value"/>
								</triple>
							</target-triples>
						</source-attribute>
					</sub-attributes>
				</source-attribute>	

				<source-attribute name="alternateName">
					<target-triples>
						<triple>
							<subject type="foaf:Person" namespace="local" prefix="pers_" idType="UUID"/>
							<predicate type="objectProperty" value="obo:ARG_2000028"/>
							<object type="vcard:Individual" namespace="local" prefix="ind_" idType="UUID"/>
						</triple>
						<triple>
							<subject type="vcard:Individual" namespace="local" prefix="ind_" idType="UUID"/>
							<predicate type="objectProperty" value="vcard:hasName"/>
							<object type="vcard:Name" namespace="local" prefix="name_" idType="UUID"/>
						</triple>
						<triple>
							<subject type="vcard:Name" namespace="local" prefix="name_" idType="UUID"/>
							<predicate type="dataProperty" value="vcard:additionalName"/>
							<object value="$value"/>
						</triple>
					</target-triples>
				</source-attribute>

				<source-attribute name="citationName">
					<target-triples>
						<triple>
							<subject type="foaf:Person" namespace="local" prefix="pers_" idType="UUID"/>
							<predicate type="objectProperty" value="obo:ARG_2000028"/>
							<object type="vcard:Individual" namespace="local" prefix="ind_" idType="UUID"/>
						</triple>
						<triple>
							<subject type="vcard:Individual" namespace="local" prefix="ind_" idType="UUID"/>
