							<object type="foaf:Person" namespace="local" prefix="pers_" idType="UUID"/>
						</triple>
						<triple>
							<subject type="vivo:AdvisingRelationship" namespace="local" prefix="advs_" idType="UUID" idSource="relation"/>
							<predicate type="dataProperty" value="rdfs:label"/>
							<object value="OrientaÃ§Ã£o de tese/dissertaÃ§Ã£o"/>
