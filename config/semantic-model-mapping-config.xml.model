				<source-attribute name="birthDate">
					<target-triples>
						<triple>
							<subject type="foaf:Person" namespace="local" prefix="pers_" idType="UUID"/>
							<predicate type="objectProperty" value="obo:ARG_2000028"/>
							<object type="vcard:Individual" namespace="local" prefix="ind_" idType="UUID"/>
