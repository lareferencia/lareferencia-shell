						<triple>
							<subject type="foaf:Person" namespace="local" prefix="pers_" idType="UUID"/>
							<predicate type="objectProperty" value="vivo:hasResearchArea"/>
							<object type="skos:Concept" namespace="local" prefix="rarea_" idType="$value"/>
